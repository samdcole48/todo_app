<h1>Tasks </h1><br />

<%= link_to("Back", "/main/index") %> <br />
<%= link_to("Add New Task", new_task_path)%>

<ul>
<% @tasks.each do |task| %>
    <li><%= task.name %></li>
    <%= link_to("Show", task_path(task)) %>
    <%= link_to("Edit", edit_task_path(task)) %>
    <%= link_to("Delete", delete_task_path(task)) %>
<% end %>
</ul>
